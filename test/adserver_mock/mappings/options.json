{
  "priority":10,
  "request": {
    "method": "OPTIONS",
    "urlPattern": ".*"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "Access-Control-Allow-Origin" : "*",
      "Access-Control-Allow-Methods" : "*",
      "Access-Control-Allow-Headers": "Accept, Content-Type, Content-Encoding, Server, Transfer-Encoding, X-Requested-With, X-Authorization, Authorization",
      "X-Content-Type-Options" : "nosniff",
      "x-frame-options" : "DENY",
      "x-xss-protection" : "1; mode=block"
    }
  }
}
