<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TS 6 - Settings" parallel="methods" thread-count="1" verbose="3">
  <test name="TC 1 - ChangeEmailNegative" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInRandomEmail"/>
          <include name="logInPopUpFirstPublisher"/>
          <include name="logInChangeEmailNegative"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC 2 - ChangePasswordNegative" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInRandomEmail"/>
          <include name="logInPopUpFirstPublisher"/>
          <include name="logInChangePasswordNegative"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC 10 - FAQ" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInRandomEmail"/>
          <include name="logInPopUpFirstPublisher"/>
          <include name="logInFAQ"/>
        </methods>
      </class>
    </classes>
  </test>
</suite>

