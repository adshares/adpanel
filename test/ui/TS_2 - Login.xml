<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TS_2 - Login Control" parallel="methods" thread-count="1" verbose="3">
  <test name="TC_1 - Poprawne logowanie" group-by-instances="true">
    <classes>
