<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TS 5 - Administration" parallel="methods" thread-count="1" verbose="3">
  <test name="TC 1 - setYourEarningTest" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInAdminEmail"/>
          <include name="logInPopUpPublisher"/>
          <include name="setYourEarningTest"/>
          <include name="logOut"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC 2 - setFilterTest" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInAdminEmail"/>
          <include name="loginPopUpAdvertiser"/>
          <include name="setFilterTest"/>
          <include name="logOut"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC 3 - ChangePassword admin" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInAdminEmail"/>
          <include name="logInPopUpPublisher"/>
          <include name="ChangePassword"/>
          <include name="logOut"/>
        </methods>
      </class>
    </classes>
  </test>
</suite>
