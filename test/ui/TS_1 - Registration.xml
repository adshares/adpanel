<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TS_1 - Registration Control" parallel="methods" thread-count="1" verbose="3">
  <test name="TC_1 - Rejestracja konta" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="Registration_RandomEmail"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC 2 - Niepoprawna rejestracja konta" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="Registration_Error"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC 3 - Resetowanie hasÅ‚a" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.AdPanel">
        <methods>
          <include name="logInRandomEmail"/>
          <include name="logInPopUpFirstPublisher"/>
          <include name="logOutTest"/>
          <include name="Registration_ForgotPassword"/>
        </methods>
      </class>
    </classes>
  </test>
</suite>

