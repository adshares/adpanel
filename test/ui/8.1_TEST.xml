<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TS - Publisher" parallel="false">

  <test name="TC_7 - Dodanie nowej Strony - podsumowanie [Site_up]" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.LoginTestCase">
        <methods>
          <include name="loginRegisterRandom"/>
          <include name="loginPopUpFirstPublisher"/>
        </methods>
      </class>
      <class name="pl.adshares.adpanel.PublisherTestCase">
        <methods>
          <include name="AddSummary"/>
        </methods>
      </class>
    </classes>
  </test>
  <test name="TC_8 - Dodanie nowej Strony - podsumowanie [Site_down]" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.LoginTestCase">
        <methods>
          <include name="loginRegisterRandom"/>
          <include name="loginPopUpFirstPublisher"/>
        </methods>
      </class>
      <class name="pl.adshares.adpanel.PublisherTestCase">
        <methods>
          <include name="AddSummary_down"/>
        </methods>
      </class>
    </classes>
  </test>

</suite>
