<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login Control Test Suite" parallel="methods" thread-count="1" verbose="3">
  <test name="Login Publisher - Correct Access" group-by-instances="true">
    <classes>
      <class name="pl.adshares.adpanel.LoginTestCase">
        <methods>
          <include name="loginTest"/>
          <include name="popUpPublisher"/>
          <include name="logOutTest"/>
        </methods>
      </class>
    </classes>
  </test>
<test name="Login Advertiser - Correct Access" group-by-instances="true">
  <classes>
    <class name="pl.adshares.adpanel.LoginTestCase">
      <methods>
        <include name="loginTest"/>
        <include name="popUpAdvertiser"/>
        <include name="logOutTest"/>
      </methods>
    </class>
  </classes>
</test>
</suite>

